	                CheckChild(pan)
	            end)

	            iZNX.ChatText("Report Bypass –≤–∫–ª—é—á–µ–Ω")
	            bypass = 1
	        else
	            hook.Remove("Think", "remove_ttt_report")
	            iZNX.ChatText("Report Bypass –≤—ã–∫–ª—é—á–µ–Ω")
	            bypass = 0
end
end }
